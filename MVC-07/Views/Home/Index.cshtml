<section class="ftco-section">
    <div class="container">
        <div class="row justify-content-center mb-5 pb-2">
            <div class="col-md-7 heading-section text-center ftco-animate">
                <h2 class="mb-4 Heading-Color">Writings</h2>
                <p>"Dive into insightful writing wonders"</p>
            </div>
        </div>
        <div class="row justify-content-center">
            <!-- Coming Soon -->
            <partial data-name="ComingSoon-LP" name="~/Views/Shared/PartialViews/ComingSoon-LP.cshtml" />
            <!-- Coming Soon -->
            <div class="col-lg-12">
                <div class="row">

                    @{
                        foreach (var w in ViewBag.MyAllWritings)
                        {
                            <div class="col-md-4">
                                <div class="blog-entry ftco-animate fadeInUp ftco-animated">
                                    <a href="#" class="img img-2" style="background-image: url(@w.IMG);"></a>
                                    <div class="text text-2 pt-2 mt-3">
                                        <span class="category mb-3 d-block"><a href="#">@w.Type - @w.Category</a></span>
                                        <h3 class="mb-4"><a href="#">@Html.Raw(w.Title)</a></h3>
                                        <p class="mb-4">@w.ShortDesription</p>

                                        <div class="meta-wrap align-items-end" style="text-align:right">

                                            <a href="#" class="btn py-2" style="padding-right:0">Continue Reading <span class="ion-ios-arrow-forward"></span></a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }
                    }

                </div>
            </div>


        </div>
    </div>
</section>

