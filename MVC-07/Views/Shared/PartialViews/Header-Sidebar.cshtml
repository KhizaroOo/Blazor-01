@{

    MVC_07.AppData.KhizoOo ME = ViewBag.ME;

}

<!-- HEADER -->
<div class="app-header header sticky">
    <div class="container-fluid main-container">
        <div class="d-flex">
            <a aria-label="Hide Sidebar" class="app-sidebar__toggle" data-bs-toggle="sidebar" href="javascript:void(0)"></a>
            <!-- LOGO -->
            <a class="logo-horizontal " href="index.html">
                <img src="~/assets/images/brand/logo-white.png" class="header-brand-img desktop-logo" alt="logo">
                <img src="~/assets/images/brand/logo-dark.png" class="header-brand-img light-logo1" alt="logo">
            </a>
            <!-- LOGO -->
            <div class="main-header-center ms-3 d-none d-lg-block"> </div>
            <div class="d-flex order-lg-2 ms-auto header-right-icons">

                <button class="navbar-toggler navresponsive-toggler d-lg-none ms-auto" type="button"
                        data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent-4"
                        aria-controls="navbarSupportedContent-4" aria-expanded="false"
                        aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon fe fe-more-vertical"></span>
                </button>
                <div class="navbar navbar-collapse responsive-navbar p-0">
                    <div class="collapse navbar-collapse" id="navbarSupportedContent-4">
                        <div class="d-flex order-lg-2">

                            <!-- LANGUAGES -->
                            <div class="d-flex country">
                                <a class="nav-link icon text-center">
                                    <i class="fe fe-globe"></i><span class="fs-16 ms-2 d-none d-xl-block">English</span>
                                </a>
                            </div>
                            <!-- LANGUAGES -->
                            <!-- THEME -->
                            <div class="d-flex">
                                <a class="nav-link icon theme-layout nav-link-bg layout-setting">
                                    <span class="dark-layout"><i class="fe fe-moon"></i></span>
                                    <span class="light-layout"><i class="fe fe-sun"></i></span>
                                </a>
                            </div>
                            <!-- THEME -->
                            <!-- FULL SCREEN -->
                            <div class="dropdown d-flex">
                                <a class="nav-link icon full-screen-link nav-link-bg">
                                    <i class="fe fe-minimize fullscreen-button"></i>
                                </a>
                            </div>
                            <!-- FULL SCREEN -->
                            <!-- PROFILE PIC -->
                            <div class="d-flex profile-1">
                                <a href="javascript:void(0)" class="nav-link leading-none d-flex">
                                    <img src="~/assets/images/users/21.jpg" alt="profile-user" class="avatar  profile-user brround cover-image">
                                </a>
                            </div>
                            <!-- PROFILE PIC -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- HEADER -->

<!-- SIDEBAR -->
<div id="khizooo-aside" class="sticky">
    <div class="app-sidebar__overlay" data-bs-toggle="sidebar"></div>
    <div class="app-sidebar">
        <div class="side-header">
            <!-- LOGO -->
            <a id="khizooo-logo" class="header-brand1" href="#">
                <img src="~/assets/images/brand/logo-white.png" class="header-brand-img desktop-logo" alt="logo">
                <img src="~/assets/images/brand/icon-white.png" class="header-brand-img toggle-logo" alt="logo">
                <img src="~/assets/images/brand/icon-dark.png" class="header-brand-img light-logo" alt="logo">
                <img src="~/assets/images/brand/logo-dark.png" class="header-brand-img light-logo1" alt="logo">
            </a>
            <!-- LOGO -->
        </div>
        <div class="main-sidemenu">
            <div class="slide-left disabled" id="slide-left">
                <svg xmlns="http://www.w3.org/2000/svg" fill="#7b8191" width="24" height="24" viewBox="0 0 24 24">
                    <path d="M13.293 6.293 7.586 12l5.707 5.707 1.414-1.414L10.414 12l4.293-4.293z" />
                </svg>
            </div>

            <ul id="khizooo-main-menu" class="side-menu">

                <li class="slide">
                    <a class="side-menu__item has-link" data-bs-toggle="slide" href="/Home">
                        <i class="side-menu__icon fe fe-home"></i><span class="side-menu__label">HOME</span>
                    </a>
                </li>

                <li class="slide">
                    <a class="side-menu__item has-link" data-bs-toggle="slide" href="/Artfulness">
                        <i class="side-menu__icon fa fa-deviantart"></i><span class="side-menu__label">ARTFULESS</span><span class="badge side-badge">100+</span>
                    </a>
                </li>

                <li class="slide">
                    <a class="side-menu__item has-link" data-bs-toggle="slide" href="/ToolsBox">
                        <i class="side-menu__icon fa fa-folder-open"></i><span class="side-menu__label">TOOLBOX</span><span class="badge side-badge">2</span>
                    </a>
                </li>

                <li class="slide">
                    <a class="side-menu__item has-link" data-bs-toggle="slide" href="/Portfolio">
                        <i class="side-menu__icon fa fa-briefcase"></i><span class="side-menu__label">PORTFOLIO</span>
                    </a>
                </li>

                <li class="slide">
                    <a class="side-menu__item has-link" data-bs-toggle="slide" href="/InfoGraphics">
                        <i class="side-menu__icon fa fa-sitemap"></i><span class="side-menu__label">INFOGRAPHICS</span><span class="badge side-badge">2</span>
                    </a>
                </li>

                <li class="slide">
                    <a class="side-menu__item has-link" data-bs-toggle="slide" href="/Writings">
                        <i class="side-menu__icon fa-solid fa-file-pen"></i><span class="side-menu__label">WRITINGS</span><span class="badge side-badge">0</span>
                    </a>
                </li>

            </ul>

            <div class="slide-right" id="slide-right">
                <svg xmlns="http://www.w3.org/2000/svg" fill="#7b8191" width="24" height="24" viewBox="0 0 24 24">
                    <path d="M10.707 17.707 16.414 12l-5.707-5.707-1.414 1.414L13.586 12l-4.293 4.293z" />
                </svg>
            </div>
        </div>
    </div>
</div>
<!-- SIDEBAR -->