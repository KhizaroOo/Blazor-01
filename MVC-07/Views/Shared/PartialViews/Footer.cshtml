            .footer-16371 .social ul li.sc a {
                color: #fffa1a !important;
            }

            .footer-16371 .social ul li.sp a {
                color: #62e7ff !important;
            }

            .footer-16371 .social ul li.tt a {
                color: #1e1b1c !important;
            }

        .footer-16371 .copyright {
            color: #999999;
        }

</style>

<footer class="footer-16371 ftco-bg-dark">
